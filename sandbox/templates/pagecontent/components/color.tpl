<div id="rank-colors" class="sandbox-block">
    <article>
        <header>
            <h5>{@css.rank_color}</h5>
        </header>
        <div class="content">
            <span class="pinned notice">{@css.notice}</span>
            <span class="pinned question">{@css.question}</span>
            <span class="pinned success">{@css.success}</span>
            <span class="pinned warning">{@css.warning}</span>
            <span class="pinned error">{@css.error}</span>
            <span class="pinned administrator">{@css.administrator}</span>
            <span class="pinned moderator">{@css.moderator}</span>
            <span class="pinned member">{@css.member}</span>
            <span class="pinned visitor">{@css.visitor}</span>
            <span class="pinned custom-author">{@css.custom.author}</span>
            <span class="pinned link-color">{@css.link.color}</span>
            <div class="spacer"></div>
            <span class="pinned bgc notice">{@css.notice}</span>
            <span class="pinned bgc question">{@css.question}</span>
            <span class="pinned bgc success">{@css.success}</span>
            <span class="pinned bgc warning">{@css.warning}</span>
            <span class="pinned bgc error">{@css.error}</span>
            <span class="pinned bgc administrator">{@css.administrator}</span>
            <span class="pinned bgc moderator">{@css.moderator}</span>
            <span class="pinned bgc member">{@css.member}</span>
            <span class="pinned bgc visitor">{@css.visitor}</span>
            <span class="pinned bgc custom-author">{@css.custom.author}</span>
            <span class="pinned bgc link-color">{@css.link.color}</span>
            <div class="spacer"></div>
            <span class="pinned bgc-full notice">{@css.notice}</span>
            <span class="pinned bgc-full question">{@css.question}</span>
            <span class="pinned bgc-full success">{@css.success}</span>
            <span class="pinned bgc-full warning">{@css.warning}</span>
            <span class="pinned bgc-full error">{@css.error}</span>
            <span class="pinned bgc-full administrator">{@css.administrator}</span>
            <span class="pinned bgc-full moderator">{@css.moderator}</span>
            <span class="pinned bgc-full member">{@css.member}</span>
            <span class="pinned bgc-full visitor">{@css.visitor}</span>
            <span class="pinned bgc-full custom-author">{@css.custom.author}</span>
            <span class="pinned bgc-full link-color">{@css.link.color}</span>
        </div>
    </article>
    <!-- Source code -->
    <div class="formatter-container formatter-hide no-js" onclick="bb_hide(this)">
        <span class="formatter-title title-perso">{@sandbox.source.code} :</span>
        <div class="formatter-content">
<pre class="language-html line-numbers"><code class="language-html">&lt;span class="pinned notice">{@css.notice}&lt;/span>
&lt;span class="pinned bgc notice">{@css.notice}&lt;/span>
&lt;span class="pinned bgc-full notice">{@css.notice}&lt;/span>
</code></pre>
        </div>
    </div>
</div>
